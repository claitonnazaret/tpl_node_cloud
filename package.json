{
  "name": "tpl_node_cloud",
  "version": "1.0.0",
  "main": "src/server.ts",
  "repository": "git@github.com:claitonnazaret/tpl_node_cloud.git",
  "author": "Claiton Nazaret <claitonnazaret@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "node dist/server.js",
    "build": "tsc && tsc-alias",
    "dev": "nodemon --watch src --exec ts-node -r tsconfig-paths/register src/server.ts -e ts",
    "husky:prepare": "husky",
    "test": "jest --passWithNoTests",
    "test:watch": "yarn test --watch",
    "test:staged": "yarn test --findRelatedTests",
    "test:push": "yarn test --coverage"
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.1",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^10.0.1",
    "git-commit-msg-linter": "^5.0.8",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^15.4.3",
    "nodemon": "^3.1.9",
    "prettier": "^3.4.2",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.23.0"
  }
}
